
@using DBShopSite.Entities
@model List<FilterItems>
<fieldset>
    <legend>@ViewBag.FilterName</legend>
    @foreach (var fi in Model)
    {
        <div>
            <input type="checkbox" class="FilterItem" id=@string.Format("{0}_{1}_{2}_" + fi.Id.ToString(),"fi","ChItem",ViewBag.FilterId) name="interest[]" value=@fi.Code>
            <label for=@fi.Code>@fi.Name</label>
        </div>
    }
</fieldset>
